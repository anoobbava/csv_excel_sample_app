<table border="1">
  <tr>
    <th>id</th>
    <th>name</th>
    <th>class_number</th>
    <th>age</th>
    <th>created_at</th>
    <th>updated_at</th>
  </tr>
    <% @users.each do |user| %>
      <tr cellspacing="50">
        <td><%= user.id%> </td>
        <td><%=user.name%></td>
        <td><%=user.class_number%></td>
        <td><%=user.age%></td>
        <td><%=user.created_at%></td>
        <td><%=user.updated_at%></td>
      </tr>
    <% end %>
</table>